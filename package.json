{
    "name": "bez-hranic-helper-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "type-check": "tsc --noEmit",
        "preview": "vite preview",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "fix": "eslint . --fix && prettier --write ."
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint",
            "prettier --check"
        ],
        "*.{json,md,html,css,scss}": [
            "prettier --check"
        ]
    },
    "prepare": "husky",
    "dependencies": {
        "d3-geo": "^3.1.1",
        "solid-js": "^1.9.3",
        "solidjs-simple-maps": "^1.3.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/d3-geo": "^3.1.0",
        "@vitest/coverage-v8": "^4.0.18",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-solid": "^0.14.5",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "sass": "^1.97.2",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.52.0",
        "vite": "^7.3.1",
        "vite-plugin-solid": "^2.11.0",
        "vitest": "^4.0.16"
    }
}
